{
    "log_format": "azure",
    "log_level": "debug",
    "storage": "memory",
    "broker": "market",
    "agents": [
        {
            "name": "paper",
            "exchange": "binance",
            "symbol": "eth-btc",
            "interval": "1h",
            "quote": "100.0",
            "restart_on_missed_candle": false,
            "strategy_config": {
                "name": "emaemacx",
                "short_period": 18,
                "long_period": 29,
                "neg_threshold": "-0.1",
                "pos_threshold": "0.1",
                "persistence": 0
            },
            "plugins": ["discord"]
        }
    ]
}
