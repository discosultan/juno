{
    "log_format": "default",
    "log_level": "debug",
    "log_outputs": [
        "stdout",
        "file"
    ],
    "storage": "sqlite",
    "broker": "market",
    "agents": [
        {
            "type": "paper",
            "exchange": "binance",
            "symbol": "eth-btc",
            "interval": "1h",
            "quote": 1.0,
            "missed_candle_policy": "ignore",
            "strategy_config": {
                "type": "emaemacx",
                "short_period": 5,
                "long_period": 7,
                "neg_threshold": -0.103,
                "pos_threshold": 0.594,
                "persistence": 3
            },
            "plugins": [
                "discord"
            ]
        }
    ]
}