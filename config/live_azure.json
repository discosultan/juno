{
    "log_format": "azure",
    "log_level": "debug",
    "storage": "sqlite",
    "broker": "market",
    "agents": [
        {
            "name": "live",
            "exchange": "binance",
            "symbol": "eth-btc",
            "interval": "1h",
            "restart_on_missed_candle": false,
            "strategy_config": {
                "name": "mamacx",
                "short_period": 18,
                "long_period": 29,
                "neg_threshold": "-0.25",
                "pos_threshold": "0.25",
                "persistence": 4
            },
            "plugins": [
                "discord"
            ]
        }
    ]
}