{
    "log_format": "azure",
    "log_level": "debug",
    "log_outputs": [
        "stdout",
        "file"
    ],
    "storage": "sqlite",
    "broker": "limit",
    "agents": [
        {
            "name": "testing_live",
            "type": "live",
            "exchange": "binance",
            "symbol": "eth-btc",
            "interval": "1h",
            "missed_candle_policy": "restart",
            "trailing_stop": 0.1832,
            "strategy_config": {
                "type": "mamacx",
                "short_period": 19,
                "long_period": 31,
                "neg_threshold": -0.201,
                "pos_threshold": 0.376,
                "persistence": 1,
                "short_ma": "smma",
                "long_ma": "ema"
            },
            "plugins": [
                "discord"
            ]
        }
    ]
}